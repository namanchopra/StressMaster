{
  "provider": "ollama",
  "model": "llama3.2:1b",
  "endpoint": "http://localhost:11434",
  "maxRetries": 3,
  "timeout": 30000,
  "options": {
    "temperature": 0.1,
    "top_p": 0.9
  },

  "_comment": "Alternative configurations:",

  "_openai_example": {
    "provider": "openai",
    "apiKey": "${AI_API_KEY}",
    "model": "gpt-3.5-turbo",
    "maxRetries": 3,
    "timeout": 30000
  },

  "_claude_example": {
    "provider": "claude",
    "apiKey": "${AI_API_KEY}",
    "model": "claude-3-sonnet-20240229",
    "maxRetries": 3,
    "timeout": 30000
  },

  "_gemini_example": {
    "provider": "gemini",
    "apiKey": "${AI_API_KEY}",
    "model": "gemini-pro",
    "maxRetries": 3,
    "timeout": 30000
  }
}
